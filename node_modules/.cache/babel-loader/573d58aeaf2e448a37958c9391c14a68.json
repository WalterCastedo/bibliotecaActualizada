{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// @ is an alias to /src\nimport ListarLibro from \"@/components/ListarLibro.vue\";\nimport ListarEstadisticas from \"@/components/ListarEstadisticas.vue\";\nimport CrearLibro from \"@/components/CrearLibro.vue\"; // import Redes from \"@/components/footeraComponent.vue\";\n//import Vue from 'vue';\n\nexport default {\n  name: \"LibrosView\",\n\n  data() {\n    return {\n      valor: 0,\n      acceso: localStorage.getItem(\"acceso\"),\n      obtenerLibros: []\n    };\n  },\n\n  components: {\n    ListarLibro,\n    CrearLibro,\n    ListarEstadisticas // Redes,\n\n  },\n  mounted: function () {\n    if (localStorage.getItem(\"acceso\") == 0) {\n      this.$router.push({\n        name: \"home\"\n      });\n    }\n  },\n  methods: {\n    variableLibro(value) {\n      this.valor = value;\n    },\n\n    listar() {\n      return this.valor === 0 ? \"active\" : \"\";\n    },\n\n    crear() {\n      return this.valor === -1 ? \"active\" : \"\";\n    },\n\n    editar() {\n      return this.valor === 1 ? \"active\" : \"\";\n    },\n\n    datos() {\n      return this.valor === 3 ? \"active\" : \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA6GA;AACA;AACA;AACA,qD,CAEA;AACA;;AAEA;EACAA,kBADA;;EAGAC;IACA;MACAC,QADA;MAEAC,sCAFA;MAGAC;IAHA;EAKA,CATA;;EAUAC;IACAC,WADA;IAEAC,UAFA;IAGAC,kBAHA,CAMA;;EANA,CAVA;EAmBAC;IAEA;MACA;QAAAT;MAAA;IACA;EAEA,CAzBA;EA4BAU;IAEAC;MACA;IACA,CAJA;;IAMAC;MACA;IACA,CARA;;IASAC;MACA;IACA,CAXA;;IAYAC;MACA;IACA,CAdA;;IAeAC;MACA;IACA;;EAjBA;AA5BA","names":["name","data","valor","acceso","obtenerLibros","components","ListarLibro","CrearLibro","ListarEstadisticas","mounted","methods","variableLibro","listar","crear","editar","datos"],"sourceRoot":"src/views","sources":["LibrosView.vue"],"sourcesContent":["<template>\n\n<div class=\"container mx-auto mt-5\">\n    <div class=\"card text-cente\">\n      <div class=\"card-header\">\n        <ul class=\"nav nav-tabs card-header-tabs\">\n          <div v-if=\"acceso==2\">\n            <li class=\"nav-item\">\n            <a\n              v-on:click=\"valor= 3\"\n              class=\"nav-link\"\n              v-bind:class=\"datos()\"\n              href=\"#\"\n              name=\"3\"\n              >Datos</a\n            >\n          </li>\n        </div>\n          <li class=\"nav-item\">\n            <a\n              v-on:click=\"valor = 0\"\n              class=\"nav-link\"\n              v-bind:class=\"listar()\"\n              aria-current=\"true\"\n              href=\"#\"\n              >Listado</a\n            >\n          </li>\n          <div v-if=\"acceso==2\">\n            <li class=\"nav-item\">\n            <a\n              v-on:click=\"valor = -1\"\n              class=\"nav-link\"\n              v-bind:class=\"crear()\"\n              href=\"#\"\n              name=\"1\"\n              >Nuevo</a\n            >\n          </li>\n        </div>\n        <div v-if=\"acceso==2\">\n          <li class=\"nav-item\">\n            <a v-if=\"valor>0 && valor!=3\"\n             \n              class=\"nav-link active\"\n             \n              href=\"#\"\n              >Editar</a\n            >\n            <a v-else\n           \n              class=\"nav-link disabled\"\n              \n              href=\"#\"\n              >Editar</a\n            >\n          </li>\n        </div>\n          \n        </ul>\n      </div>\n      <div class=\"card-body bg-white\">\n        <!-- <CrearLibro v-show=\"valor==1\" variable=1></CrearLibro>\n       <CrearLibro v-show=\"valor==2\" variable=2></CrearLibro> -->\n\n        \n    <CrearLibro\n      @recibiendo=\"variableLibro\"\n      v-show=\"valor == -1\"\n      v-bind:variable=\"valor\"\n    ></CrearLibro>\n    <CrearLibro\n      @recibiendo=\"variableLibro\"\n      v-if=\"valor > 0 && valor!=3\"\n      v-bind:variable=\"valor\"\n    ></CrearLibro>\n    <ListarLibro v-show=\"valor == 0\"\n      @recibiendo=\"variableLibro\"\n      v-if=\"valor == 0\"\n      v-bind:variable=\"valor\"\n      class=\"mx-2 mx-sm-5\"\n    >\n    </ListarLibro>\n   \n    <ListarEstadisticas\n    @recibiendo=\"variableLibro\"\n      v-if=\"valor == 3 \"\n      v-bind:variable=\"valor\"\n    >\n    </ListarEstadisticas>\n\n\n\n\n      </div>\n    </div>\n  </div>\n\n\n\n\n\n\n\n</template>\n\n<style></style>\n\n<script>\n// @ is an alias to /src\nimport ListarLibro from \"@/components/ListarLibro.vue\";\nimport ListarEstadisticas from \"@/components/ListarEstadisticas.vue\";\nimport CrearLibro from \"@/components/CrearLibro.vue\";\n\n// import Redes from \"@/components/footeraComponent.vue\";\n//import Vue from 'vue';\n\nexport default {\n  name: \"LibrosView\",\n\n  data() {\n    return {\n      valor: 0,\n      acceso: localStorage.getItem(\"acceso\"),\n      obtenerLibros: [],\n    };\n  },\n  components: {\n    ListarLibro,\n    CrearLibro,\n    ListarEstadisticas,\n\n\n    // Redes,\n  },\n\n  mounted: function () {\n   \n    if (localStorage.getItem(\"acceso\") ==0) {\n      this.$router.push({ name: \"home\" });\n    }\n\n  },\n  \n\n  methods: {\n    \n    variableLibro(value) {\n      this.valor = value;\n    },\n   \n    listar() {\n      return this.valor === 0 ? \"active\" : \"\";\n    },\n    crear() {\n      return this.valor === -1 ? \"active\" : \"\";\n    },\n    editar() {\n      return this.valor === 1 ? \"active\" : \"\";\n    },\n    datos() {\n      return this.valor === 3 ? \"active\" : \"\";\n    },\n  \n  },\n};\n</script>\n\n"]},"metadata":{},"sourceType":"module"}