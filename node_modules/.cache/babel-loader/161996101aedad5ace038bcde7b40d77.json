{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// @ is an alias to /src\nimport ListarAutores from \"@/components/ListarAutores.vue\";\nimport CrearAutores from \"@/components/CrearAutor.vue\";\nexport default {\n  name: \"AutoresView\",\n\n  data() {\n    return {\n      valor: 0,\n      name: 0\n    };\n  },\n\n  components: {\n    ListarAutores,\n    CrearAutores\n  },\n  mounted: function () {\n    if (localStorage.getItem(\"acceso\") != 2) {\n      this.$router.push({\n        name: \"home\"\n      });\n    }\n  },\n  created: function () {\n    this.valor = 0;\n  },\n  methods: {\n    listar() {\n      return this.valor === 0 ? \"active\" : \"\";\n    },\n\n    crear() {\n      return this.valor === -1 ? \"active\" : \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAuCA;AACA;AACA;AAEA;EACAA,mBADA;;EAGAC;IACA;MACAC,QADA;MAEAF;IAFA;EAIA,CARA;;EASAG;IACAC,aADA;IAEAC;EAFA,CATA;EAaAC;IACA;MACA;QAAAN;MAAA;IACA;EACA,CAjBA;EAmBAO;IACA;EACA,CArBA;EAuBAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;IACA;;EANA;AAvBA","names":["name","data","valor","components","ListarAutores","CrearAutores","mounted","created","methods","listar","crear"],"sourceRoot":"src/views","sources":["AutoresView.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto mt-5\">\n    <br/>\n    <div class=\"card text-cente\">\n      <div class=\"card-header\">\n        <ul class=\"nav nav-tabs card-header-tabs\">\n          <li class=\"nav-item\">\n            <a\n              v-on:click=\"valor = 0\"\n              class=\"nav-link\"\n              v-bind:class=\"listar()\"\n              aria-current=\"true\"\n              href=\"#\"\n              >Listado</a\n            >\n          </li>\n          <li class=\"nav-item\">\n            <a\n              v-on:click=\"valor = -1\"\n              class=\"nav-link\"\n              v-bind:class=\"crear()\"\n              href=\"#\"\n              name=\"1\"\n              >Nuevo</a\n            >\n          </li>\n     