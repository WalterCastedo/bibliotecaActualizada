ableLibro(value) {\n      this.valor = value;\n    },\n   \n    listar() {\n      return this.valor === 0 ? \"active\" : \"\";\n    },\n    crear() {\n      return this.valor === -1 ? \"active\" : \"\";\n    },\n    editar() {\n      return this.valor === 1 ? \"active\" : \"\";\n    },\n  \n  },\n};\n</script>\n\n"]},"metadata":{},"sourceType":"module"}