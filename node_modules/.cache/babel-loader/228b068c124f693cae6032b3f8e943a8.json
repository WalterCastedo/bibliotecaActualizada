{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ListarEstadisticasS from \"@/components/ListarEstadisticasSolicitudes.vue\";\nimport Listarsolicitudes from \"@/components/ListasolicitudesComponent.vue\";\nimport ListarsolicitudesAdmin from \"@/components/ListasolicitudesAdmin.vue\";\nexport default {\n  name: \"solicitudesView\",\n\n  data() {\n    return {\n      valor: 0,\n      acceso: localStorage.getItem(\"acceso\")\n    };\n  },\n\n  components: {\n    Listarsolicitudes,\n    ListarsolicitudesAdmin,\n    ListarEstadisticasS\n  },\n\n  mounted() {\n    if (localStorage.getItem(\"acceso\") == 0) {\n      this.$router.push({\n        name: \"home\"\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":";AAgEA;AACA;AACA;AAEA;EACAA,uBADA;;EAEAC;IACA;MACAC,QADA;MAEAC;IAFA;EAIA,CAPA;;EAQAC;IACAC,iBADA;IAEAC,sBAFA;IAGAC;EAHA,CARA;;EAaAC;IACA;MACA;QAAAR;MAAA;IACA;EACA;;AAjBA","names":["name","data","valor","acceso","components","Listarsolicitudes","ListarsolicitudesAdmin","ListarEstadisticasS","mounted"],"sourceRoot":"src/views","sources":["solicitudesView.vue"],"sourcesContent":["<template>\n    <div class=\"container mx-auto mt-5\">\n      <div class=\"card text-cente\">\n      <div class=\"card-header\">\n        <ul class=\"nav nav-tabs card-header-tabs\">\n          <div v-if=\"acceso==2\">\n            <li class=\"nav-item\">\n            <a\n              v-on:click=\"valor= 1\"\n              class=\"nav-link\"\n              v-bind:class=\"datos()\"\n              href=\"#Solicitudes\"\n              name=\"3\"\n              >Datos</a\n            >\n          </li>\n        </div>\n          <li class=\"nav-item\">\n            <a\n              v-on:click=\"valor = 0\"\n              class=\"nav-link\"\n              \n              v-bind:class=\"listar()\"\n              aria-current=\"true\"\n              href=\"#Solicitudes\"\n              >Listado</a\n            >\n          </li>\n          </ul> \n        </div> \n        <transition name=\"slide-fade\" mode=\"out-in\">\n        <div class=\"card-body bg-white\">\n          <!-- Contenido de los componentes con transición -->\n          <div v-if=\"acceso == 2\">\n            <ListarEstadisticasS v-show=\"valor == 1\" v-bind:variable=\"valor\" />\n          </div>\n          <div v-if=\"acceso == 1\">\n            <Listarsolicitudes v-show=\"valor == 0\" v-bind:variable=\"valor\" />\n          </div>\n          <div v-else>\n            <ListarsolicitudesAdmin v-show=\"valor == 0\" v-bind:variable=\"valor\" />\n          </div>\n        </div>\n      </transition>\n    </div>\n  </div>\n</template>\n\n<style>\n/* Define los estilos para la transición */\n.slide-fade-enter-active {\n  transition: all 0.3s ease-out;\n}\n.slide-fade-leave-active {\n  transition: all 0.3s cubic-bezier(1.0, 0.5, 0.8, 1.0);\n}\n.slide-fade-enter,\n.slide-fade-leave-to {\n  opacity: 0;\n  transform: translateX(10px);\n}\n</style>\n\n<script>\nimport ListarEstadisticasS from \"@/components/ListarEstadisticasSolicitudes.vue\";\nimport Listarsolicitudes from \"@/components/ListasolicitudesComponent.vue\";\nimport ListarsolicitudesAdmin from \"@/components/ListasolicitudesAdmin.vue\";\n\nexport default {\n  name: \"solicitudesView\",\n  data() {\n    return {\n      valor: 0,\n      acceso: localStorage.getItem(\"acceso\"),\n    };\n  },\n  components: {\n    Listarsolicitudes,\n    ListarsolicitudesAdmin,\n    ListarEstadisticasS,\n  },\n  mounted() {\n    if (localStorage.getItem(\"acceso\") == 0) {\n      this.$router.push({ name: \"home\" });\n    }\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}