dmin,\n    ListarEstadisticasS,\n  },\n  computed: {\n    componentToShow() {\n      if (this.acceso == 2 && this.valor === 1) {\n        return 'ListarEstadisticasS';\n      } else if (this.acceso == 1 && this.valor === 0) {\n        return 'Listarsolicitudes';\n      } else {\n        return 'ListarsolicitudesAdmin';\n      }\n    }\n  },\n};\n</script>\n\n<style scoped>\n.fade-enter-active,\n.fade-leave-active {\n  transition: opacity 10s;\n}\n\n.fade-enter,\n.fade-leave-to {\n  opacity: 0;\n}\n</style>"]},"metadata":{},"sourceType":"module"}